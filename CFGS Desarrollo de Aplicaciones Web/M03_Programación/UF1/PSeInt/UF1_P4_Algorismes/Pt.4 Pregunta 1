Algoritmo múltiplos_2_3
	i <- 1
	j <- 0
	k <- 0
	Para i<-1 Hasta 100 Hacer
		Si i MOD 2==0 Entonces
			Si i MOD 3==0 Entonces
				k <- k+1
			FinSi
			j <- j+1
		SiNo
			Si i MOD 3==0 Entonces
				k <- k+1
			FinSi
		FinSi
	FinPara
	Escribir 'Del 1 al 100 hay ',j,' múltiplos de 2 y ',k,' múltiplos de 3.'
FinAlgoritmo

