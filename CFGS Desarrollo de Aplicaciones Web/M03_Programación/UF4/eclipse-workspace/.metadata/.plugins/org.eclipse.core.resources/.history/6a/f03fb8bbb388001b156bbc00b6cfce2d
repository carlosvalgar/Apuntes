
public class Discos {
	private int codigo;
	private String titulo;
	private String autor;
	private String nomDiscografica;
	
	// Constructores
	
	Discos() {}
	
	Discos(int codigo, String titulo) {
		this.codigo = codigo;
		this.titulo = titulo;
	}
	
	Discos(int codigo, String titulo, String autor) {
		this.codigo = codigo;
		this.titulo = titulo;
		this.autor = autor;
	}
	
	Discos(int codigo, String titulo, String autor, String nomDiscografica) {
		this.codigo = codigo;
		this.titulo = titulo;
		this.autor = autor;
		this.nomDiscografica = nomDiscografica;
	}
	
	// Setters
	
	
	// Getters
	public void GetCodigo() {
		return this.codigo;
	}
	
	
	
	
	
}
