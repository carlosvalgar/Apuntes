
public class Discos {
	private int codigo;
	private String titulo;
	private String autor;
	private String nomDiscografica;
	
	// Constructores
	
	Discos() {}
	
	Discos(int codigo, String titulo) {
		this.codigo = codigo;
		this.titulo = titulo;
	}
	
	Discos(int codigo, String titulo, String autor) {
		this.codigo = codigo;
		this.titulo = titulo;
		this.autor = autor;
	}
	
	Discos(int codigo, String titulo, String autor, String nomDiscografica) {
		this.codigo = codigo;
		this.titulo = titulo;
		this.autor = autor;
		this.nomDiscografica = nomDiscografica;
	}
	
	// Setters
	public void SetCodigo(int codigo) {
		this.codigo = codigo;
	}
	
	public void SetTitulo(String titulo) {
		this.titulo = titulo;
	}
	
	public void SetAutor(String autor) {
		this.autor = autor;
	}
	
	public void SetNomDiscografica(String nomDiscografica) {
		this.nomDiscografica = nomDiscografica;
	}
	
	// Getters
	public int GetCodigo() {
		return this.codigo;
	}
	
	public String GetTitulo() {
		return this.titulo;
	}
	
	public String autor() {
		return this.autor;
	}
	
	public String nomDiscografica() {
		return this.nomDiscografica;
	}
	
	@Override
	public String toString(){
		String cadena = "Codigo: " + this.codigo + "\nTitulo: " + this.titulo + "\nAutor: " + this.autor + "\nNombre de Discográfica: " + this.nomDiscografica;
		return cadena;
	}
	
}
